{
  "$schema": "http://json.schemastore.org/template",
  "author": "Andrew G",
  "classifications": [ "Robocode", "Game", "Bot" ],
  "identity": "RobocodeBot",
  "name": "Robocode Tank Royale Bot",
  "shortName": "robocodebot",
  "sourceName": "MyFirstRoboBot",
  "tags": {
    "language": "C#",
    "type": "project",
    "sources": [
    {
      "source": "./",
      "target": "./",
      "exclude": [
        "**/*.nuspec",
        "**/*.nupkg",
        "_rels/**",
        "[Content_Types].xml",
        "package/**",
        ".signature.p7s"
      ]
    }
  ]
  },
  "symbols": {
    "BotName": {
      "type": "parameter",
      "datatype": "string",
      "defaultValue": "MyFirstRoboBot",
      "replaces": "MyFirstRoboBot",
      "description": "Bot name, namespace, and project name"
    }
  }
}
